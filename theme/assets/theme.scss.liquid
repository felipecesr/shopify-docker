==============================================================================*/


.template-index header.hero-active {

  &.hero-active--full_screen {
    height: 100vh;
  }

  .hero-image-header-wrap {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    padding: 0;
    z-index: 2;
