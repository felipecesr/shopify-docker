==============================================================================*/

.modal {
  background-color: {{ settings.color_body_bg | color_modify: 'alpha', 0.96 }};
  color: $colorText;
  display: flex;
  opacity: 0;
  pointer-events: none;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  padding: $gutter;
  justify-content: center;
  align-items: center;
  transition: all 50ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  pointer-events: none;
}

.modal--is-active {
  @include transform(translateY(0));
  opacity: 1;
  overflow: hidden;
  z-index: 3;
  pointer-events: auto;
}

.modal__inner {
  @include prefix(transform-style, preserve-3d, moz webkit spec);
  @include transform(translateY(20px) translate3d(0,0,0));
  background-color: {{ settings.color_body_bg }};
  box-shadow: 0 0 10px {{ settings.color_text | color_modify: 'alpha', 0.07 }};
  height: auto;
  padding: 5%;
  overflow-y: auto;
  position: relative;
  opacity: 0;
  transition: transform 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95),
              opacity 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
}

.modal--is-active .modal__inner {
  @include transform(translateY(0));
  opacity: 1;
}

.modal__centered {
  @include transform(translateY(-50%));
  position: relative;
  top: 50%;

  .no-csstransforms & {
    top: 20%;
  }
}

.modal__close {
  position: absolute;
  border: 0;
  padding: ($gutter / 2) ($gutter / 1.5);
  top: 0;
  right: 0;
  display: flex;
  justify-content: center;
  align-items: center;

