/*============================================================================
  #Site Footer
==============================================================================*/
.site-footer {
  background-color: $colorFooterBg;
  padding: 0 0 $gutter 0;
  color: $colorFooterText;

  @include at-query ($min, $large) {
    padding: 0 0 ($gutter * 2) 0;
  }

  hr.border-top {
    padding-bottom: $gutter;
    &.border-top--none {
      border: none;
    }
  }


  h3.secondary {
    color: $colorFooterSecondary;
    margin-bottom: $gutter;
  }

  .footer-newsletter {
    &:not(:last-of-type) {
      padding-right: $gutter;
    }

    @include at-query ($max, $small) {
      padding-right: 0;
    }

    .input-group {
      width: auto;
      display: flex;
      justify-content: space-between;

      @include at-query ($max, $small) {
        display: block;
        input {
          margin-bottom: 8px;
        }
        .input-group-btn {
          padding-left: 0;

          .btn {
            width: 100%;
          }
        }
      }
    }

    form {
      margin-top: -8px;
    }

    input.input-group-field {
      border-color: $colorFooterSecondary;
      color: $colorFooterText;

      @include at-query ($min, $postSmall) {
        flex: 1 1 auto;
        min-width: 20%;
        max-width: 100%;
      }

    }

    .input-group-btn {
      padding-left: 12px;

      @include at-query ($min, $postSmall) {
        flex: 1 0 auto;
        min-width: 20%;
        max-width: 100%;
        width: auto;
      }

      .btn {
        width: 100%;
        {% if settings.button_style == 'full_rounded' or settings.button_style == 'full_square' %}
          background: $colorFooterText;
          color: $colorFooterBg;
          border: none;
        {% else %}
          border: 2px solid $colorFooterText;
          color: $colorFooterText;
        {% endif %}

        {% if settings.button_style == 'full_rounded' or settings.button_style == 'border_rounded' %}
         border-radius: 6px;
        {% endif %}

        &:hover,
        &:active,
        &:focus {
          outline: none;
          {% if settings.button_style == 'full_rounded' or settings.button_style == 'full_square' %}
          background-color: $colorBtn;
          color: $colorBtnText;
          {% else %}
          color: $colorBtn;
          border: 2px solid $colorBtnText;
          {% endif %}
        }

      }
    }

    /*================ Placeholders ================*/
    $colorFooterInputPlaceholder: rgba($colorFooterText, .31);

    ::-webkit-input-placeholder { /* Chrome/Opera/Safari */
      color: $colorFooterInputPlaceholder;
    }
    ::-moz-placeholder { /* Firefox 19+ */
      color: $colorFooterInputPlaceholder;
    }
    :-ms-input-placeholder { /* IE 10+ */
      color: $colorFooterInputPlaceholder;
    }
    :-moz-placeholder { /* Firefox 18- */
      color: $colorFooterInputPlaceholder;
    }
  }

  .footer-nav {
    @include featureCopyFontType();
    margin: 0;
    padding: 0;
    position: relative;
    li {
      display: inline-block;
      margin-right: ($gutter/2);
      a {
        color: $colorFooterText;
        &:hover,
        &:active {
          color: $colorAccent;
        }
      }
      .site-nav__dropdown li a {
        padding-right: $gutter/2;
      }
    }
  }

  .footer-meta {
    color: $colorFooterSecondary;
    font-family: $secondaryFontStack;
    a {
      color: $colorFooterSecondary;
      &:hover,
      &:active {
        color: $colorAccent;
      }
    }
    p.sub-meta {
      font-size: 0.8em;
    }
    p {
      margin-top: 7.5px;
      margin-bottom: 7.5px;
    }
  }

  $payment-icon-spacing: 3px;

  .payment-icons {
    margin: ($gutter / 2) ($payment-icon-spacing * -1);

    @include at-query($max, $medium) {
      margin-top: ($gutter / 4);
    }

    font-size: 0;
    line-height: 1;

    li {
      margin: 0 $payment-icon-spacing;
      color: $colorFooterSecondary;

      .payment-icon {
        width: 40px;
        height: auto;
        display: block;
      }
    }
  }

  .social-footer {
    margin: 0;
    li {
      margin: 0 15px 15px 0;
      a {
        color: $colorFooterSocial;
        &:hover,
        &:active {
          color: $colorAccent;
        }
      }
    }
  }

  .footer-top-row {
    margin-bottom: ($gutter * 2);
  }


  @include at-query ($max, $desktop) {

    .footer-top-row {
      margin-bottom: 0;
      > .grid__item {
        margin-bottom: ($gutter *2);
      }
    }

    .footer-grid-nav {
      width: 100%;
    }

    .footer-grid-newsletter {
      width: 50%;
    }

    .footer-grid-social {
      width: 50%;
    }

    .payment-icons {
      text-align: left;
    }
  }

  @include at-query ($max, $medium) {


    .footer-grid-newsletter {
      width: 100%;
    }

    .footer-grid-social {
      width: 100%;
    }
  }

  .bottom-row {
    margin-top: ($gutter / 2);
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
  }

  .bottom-row__copyright,
  .bottom-row__payment-and-locale {
    -webkit-box-flex: 1;
    -ms-flex: 1 1 0px;
    flex: 1 1 0%;
    min-width: 0;
  }

  .bottom-row__payment-and-locale {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -ms-flex-direction: column;
    flex-direction: column;
  }

  @include at-query ($max, $medium) {
    .bottom-row {
      -webkit-box-orient: vertical;
      -webkit-box-direction: reverse;
      -ms-flex-direction: column-reverse;
      flex-direction: column-reverse;
      -webkit-box-align: center;
      -ms-flex-align: center;
      align-items: center;
    }

    .bottom-row__payment-and-locale {
      -webkit-box-orient: vertical;
      -webkit-box-direction: reverse;
      -ms-flex-direction: column-reverse;
      flex-direction: column-reverse;
    }

    .locale-selectors .disclosure-list {
      left: 0;
      right: auto;
    }
  }

  .locale-selectors {
    margin-bottom: ($gutter / 2);
  }

  .selectors-form {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-box-pack: end;
    -ms-flex-pack: end;
    justify-content: flex-end;

    @include at-query ($max, $small) {
      -webkit-box-pack: center;
      -ms-flex-pack: center;
      justify-content: center;
    }
  }

  .selectors-form__item {
    margin-top: ($gutter / 4);
  }

  .selectors-form__item:not(:first-of-type) {
    margin-left: 6px;
  }

  .disclosure__toggle {
    color: $colorFooterText;
  }

  .disclosure-list {
    background-color: $colorFooterBg;
  }

  .disclosure-list__item:active {
    .disclosure-list__option {
      color: $colorFooterBg;
    }
  }


  .disclosure-list__option {
    color: $colorFooterText;

    &:focus,
    &:hover {
      color: $colorFooterText;
    }
  }

