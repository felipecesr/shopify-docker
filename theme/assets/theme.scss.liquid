/*============================================================================
  #Drawer Nav
  - List of items inside the mobile drawer
==============================================================================*/
.drawer-nav {
  // Negative of .drawer left/right padding for full-width link tap area
  margin: (-$gutter / 2) (-$gutter / 2) 0 (-$gutter / 2);

  li {
    margin-bottom: 0;
  }
}

.drawer__container {
  min-height: 101%;
}

.drawer-nav__search {
  padding: $gutter $gutter / 2;

  .search-bar {
    margin-bottom: 0;
  }
}

.drawer-nav__item {
  position: relative;
  display: block;
  cursor: pointer;
  // Background color on top level items so there is no
  // element overlap on subnav's CSS toggle animation
  .drawer-nav > & {
    background-color: $colorDrawerBackground;
  }

  &:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: $gutter / 2;
    right: $gutter / 2;
  }

  .drawer-nav > &:last-child:after {
    display: none;
  }


}

// Login/logout links can't have a class on them, so style <a> element
.drawer-nav__item a {
  display: block;
}

.drawer-nav__item a,
.drawer-nav__toggle button {
  color: $colorDrawerNav;;
  padding: $gutter / 2;
  text-decoration: none;

  &:hover,
  &:active,
  &:focus {
    color: $colorDrawerAccent;
  }

}

.drawer-nav__item .drawer-nav__has-sublist a {
  &:hover,
  &:active,
  &:focus {
    color: $colorDrawerAccent;
  }
}

.drawer-nav__item.customer-item {
  &.first {
    margin-top: ($gutter/2);
  }
  a {
    padding-top: $gutter/4;
    padding-bottom: $gutter/4;
    color: $colorSecondary;
    &:hover,
    &:active,
    &:focus {
      color: $colorDrawerAccent;
    }
  }
}


.drawer-nav__item--active {
  color: $colorAccent;
}

.drawer-nav__has-sublist,
.drawer-nav--child__has-sublist {
  display: table;
  width: 100%;

  .drawer-nav__link {
    display: table-cell;
    vertical-align: middle;
    width: 100%;
    .icon {
      float: right;
      font-size: 24px;
      padding-left: $gutter / 4;
    }
    .icon-expand_less {
      display: none;
    }
    .icon-expand_more {
      display: block;
    }
  }

  &.drawer-nav--expanded {
    .drawer-nav__link {
      .icon-expand_less {
        display: block;
      }
      .icon-expand_more {
        display: none;
      }
    }
  }
}


.drawer-nav__toggle {
  display: table-cell;
  vertical-align: middle;
  width: 1%;
}

.drawer-nav__toggle-open {
  .drawer-nav--expanded & {
    display: none;
  }
}

.drawer-nav__toggle-close {
  display: none;

  .drawer-nav--expanded & {
    display: block;
  }
}

.drawer-nav__sublist {
  margin: 0;
  max-height: 0;
  visibility: hidden;
  overflow: hidden;
  transition: all 300ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  @include backface();

  .drawer-nav--expanded + & {
    visibility: visible;
    max-height: 5000px;
    transition: all 700ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  }

  &.large-sublist {
    .drawer-nav--expanded + & {
      max-height: 10000px;
    }

  }

  .drawer-nav__item a {
    padding: ($gutter/3) 0 ($gutter/3) ($gutter/2);
  }

  .drawer-nav__item:after {
    top: 0;
    bottom: auto;
  }

  .drawer-nav__link {
    padding-left: $gutter;
    font-weight: normal;
    span {
      @include opacity(0.3);
      display: inline-block;
      margin-right: 8px;
    }

  }
}

//child sublist
.drawer-nav__sublist .drawer-nav__item .drawer-nav--child__has-sublist {
  a {
    padding: ($gutter/3) ($gutter/2);
  }
  span.icon-arrow-down {
    @include opacity(1);
    margin: 0;
  }

}

.drawer-nav__sublist .drawer-nav__item .drawer-nav__link--child {
  font-size: em(12px);
  padding-left: 40px;
}


