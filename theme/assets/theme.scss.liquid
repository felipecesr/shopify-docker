.lt-ie9 section.homepage-tour {

  display: none;

}

.tour-item-wrapper--page {
  padding: 0;
}


/*============================================================================
  #Blog
==============================================================================*/

section.homepage-blog, section.blog  {

  h3.standard-index, h3.standard-single {
    margin-bottom: 0;
    a {
      color: $colorTitle;
      &:hover {
        color: $colorAccent;
      }
    }
  }

  .article-image {
    height: 300px;
    background-size: cover !important;
    margin-bottom: ($gutter/2);
    @include opacity(1);
    @include transition(opacity,0.2s,ease-in-out);
    @include at-query($max, $desktop) {
      height: 180px;
    }
  }

  a:hover {
    .article-image {
      @include opacity(0.6);
    }
  }

  time.subheader {
    margin-bottom: ($gutter/2);
    display: block;
  }

  @include at-query($max, $medium) {
    &.homepage-blog {
      .grid__item {
        margin-bottom: $gutter;
      }
    }
  }

  p.read-more {
    margin-bottom: 0;
  }

}

section.blog {

  .blog-header {
    margin-bottom: ($gutter*2);
    padding-bottom: ($gutter/2);
    border-bottom: 1px solid $colorBorder;
    .standard-index {
      margin: 0;
    }
  }

  .article-item {
    margin-bottom: ($gutter*2);
    padding-bottom: ($gutter*2);
  }

  .article-image-wrapper {
    margin-bottom: $gutter*1.5;
  }

  .feature-image {
    margin-bottom: $gutter;
  }

  hr {
    color: $colorBorder;
    height: 1px;
    margin: ($gutter*2) 0 0 0;
  }

  h3.standard-single, h1.standard-single {
    margin-bottom: $gutter;
    @include at-query($max, $desktop) {
      margin-bottom: 8px;
    }
  }

  .date, .author {
    &.date {
      margin-bottom: 4px;
      @include featureCopyFontType();
    }
    &.author {
      margin-bottom: $gutter;
    }
  }

  .article-meta, .meta {
    color: $colorSecondary;
    a {
      color: $colorSecondary;
      &:hover {
        color: $colorAccent;
      }
    }
    &.meta {display:none;}
  }

  .article-content {
    @include at-query($max, $desktop) {
      width: 100%;
    }
  }

  a.read-more {
    .next-arrow {
      svg {
        vertical-align: -2px;
      }
    }
  }

  .subtitle {
    display: none;
    @include featureCopyFontType();
    color: $colorSecondary;
    margin-bottom: $gutter;
  }

  .blog-sidebar {

    h4 {
      text-transform: none;
      font-weight: 500;
      @include featureCopyFontType();
      font-size: 1em;
      color: $colorSecondary;
      &.tags {
        margin-top: $gutter;
      }
    }

    li {
      margin-bottom: $gutter/2;
    }

    .tags-list {
      li {
        color: $colorAccent;
        margin-bottom: 4px;
        a {
          color: $colorText;
          &:hover {
            color: $colorAccent;
          }
        }
      }
    }

    p.rss-link {
      margin-top: $gutter;
      a {
        color: $colorSecondary;
        &:hover {
          color: $colorAccent;
        }
      }
    }
  }
}

section.blog.blog-article {

  .comments-holder {
    padding-top: $gutter*2;
    > h3 {
      margin-bottom: $gutter;
    }
    .subtitle {
      text-transform: none;
      font-weight: 500;
      display: block;
    }
    hr {
      margin: ($gutter*2) 0;
    }
  }

  .pagination {
    margin-bottom: 0;
  }

  .comments-hr {
    margin-top: 0;
  }

  .share-links {
    margin-top: $gutter;
    li {
      margin-right: 8px;
    }
  }
}

/*============================================================================
  #Product Page
==============================================================================*/
section.section-product {

  .template-product & {
    margin: ($gutter*2) 0 0;
    padding: 0;
  }

  .product-single__variants {
    display: none;

    .no-js & {
      display: block;
    }
  }

  .product-single__media {
    position: relative;

    img,
    video,
    iframe,
    .shopify-model-viewer-ui,
    .plyr,
    .media-item,
    .video-wrapper {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      max-width: 100%;
      height: 100%;
    }

    .shopify-model-viewer-ui model-viewer,
    .plyr .plyr__video-wrapper {
      width: 100%;
      height: 100%;
    }
  }

  .product-single__media--video {
    background-color: $color-video-bg;
  }

  .hidden_lightbox_images {
    display: none;
  }

  .product-single__media,
  .product-single__thumbnails {
    a, img {
      display: block;
      margin: 0 auto;
      width: 100%;
    }

    li {
      margin-bottom: $gutter;
      float: none;
      display: inline-block;
      margin-right: -4px;
    }

    a.product-single__thumbnail {
      @include opacity(0.6);
      @include transition(opacity,0.1s,ease-in-out);
      position: relative;
      display: block;

      &.active,
      &:hover {
        @include opacity(1);
      }

      .media-badge-container {
        position: absolute;
        top: 0;
        right: 0;
        padding-top: ($gutter / 6);
        padding-right: ($gutter / 6);
        width: calc(100% / 3);
        // Set a min-width to meet UX requirements.
        // 20px badge + 5px padding = 25px
        min-width: 25px;
      }
    }
  }

  .product__placeholder {
    background-color: #DCDCDC;
  }

  .product-single__thumbnails {
    margin: $gutter 0 0;
    padding: 0;
    li {
      padding: 8px;
    }
  }

  h1 {
    margin-bottom: 0;
    color: $colorTitle;
  }

  .priceWrapper {
    margin: 8px 0 24px 0;
  }

  &.has-unit-price .priceWrapper,
  &.has-taxes-or-shipping-policy-text:not(.has-unit-price) .priceWrapper {
    margin: 8px 0 8px 0;
  }

  #ProductPrice {
    font-weight: 500;
    color: $colorSubheader;
    display: inline-block;
  }

  #ComparePrice {
    display: inline-block;
    color: $colorSecondary;
    margin-left: $gutter/2;
    font-weight: 500;
  }

  .unitPriceWrapper {
    margin: 8px 0 32px 0;
  }

  &.has-taxes-or-shipping-policy-text .unitPriceWrapper {
    margin: 8px 0 8px 0;
  }

  #unitPrice {
    font-size: em(16px);
  }

  .taxAndShippingPolicyWrapper {
    margin: 0 0 32px;
  }

  .cta-content {
    margin-top: -($gutter/2);
    margin-bottom: 28px;
    overflow: auto;

    .cta-btn-container {
      // width: 100%;
    }

    .cta-btn-container, .dynamic-payment-buttons {
        display: inline-block;

        @include at-query ($max, $small) {
          display: block;
          width: 100%;
        }
    }

    .AddToCart, #AddToCart {
      margin: ($gutter / 2) ($gutter / 2) 0 0;
      min-height: 44px;

      @include at-query ($max, $small) {
        width: 100%;
        margin-left: 0;
        margin-right: 0;
      }
    }
    .cta-complete {
      float: left;
      margin: ($gutter / 2) 0 0 0;
      width: 100%;
      padding: 7.5px 0 8.5px;
      display: none;
      @include opacity(0);
      @include transition(opacity,0.2s,ease-in-out);
      &.visible {
        display: block;
      }
      &.show {
        @include opacity(1);
      }
    }
  }

  .product-info {
    h1 {
      @include at-query ($max, $medium) {
        margin-top: ($gutter/2);
        padding-top: $gutter;
        border-top: 1px solid $colorBorder;
      }
    }

    label {
      {% if settings.hide_drop_down_labels %}
        display: none !important;
      {% endif %}
    }

    .quantity-selector {
      width: $gutter*2;
    }

  }

  .product-description {
    padding: $gutter 0;
    border-top: 1px solid $colorBorder;
    margin-bottom: 0;
  }


  .product-share-links {
    padding: $gutter 0;
    border-top: 1px solid $colorBorder;
    .share-links {
      padding: 0;
      margin: 0;
      list-style-type: none;
      li {
        display: inline-block;
        margin-right: 8px;
        .h5 {
          font-weight: normal;
        }
        a {
          color: $colorSecondary;
          padding: 8px;
          font-size: em(18px);
          svg {
            vertical-align: -2px;
          }
          &:hover,
          &:focus {
            color: $colorAccent;
          }

        }
      }
    }
  }

  .product-pagination {
    padding: $gutter 0;
    border-top: 1px solid $colorBorder;
    a {
      @include secondaryType();
      color: $colorSecondary;
      padding: 8px;
      &.previous-paginate {
        padding-left: 0;
      }
      &.next-paginate {
        padding-right: 0;
      }
      &:hover,
      &:focus {
        color: $colorAccent;
      }

    }
  }
}

#swipebox-bottom-bar {
  display: none !important;
}


// Shopify Pay

//shopify pay - make primary
.cta-content--pay-buttons {

  .shopify-payment-button,
  .btn {
    margin-top: ($gutter/2);
    vertical-align: top;
    min-width: 304px;

    @include at-query ($max, $small) {
      width: 100%;
      margin-left: 0;
      margin-right: 0;
      min-width: unset;
    }
  }
  .shopify-payment-button {
    display: inline-block;
  }
  .shopify-payment-button__button,
  .shopify-payment-button__button--unbranded {
    min-height: 44px;
    width: 100%;

    @extend .btn--accent;
  }

  .shopify-payment-button__button--branded {
    border: none;
    &:hover,
    &:active,
    &:focus {
      border: none;
    }
  }

  .shopify-payment-button__button--unbranded  {
    &[disabled],
    &.disabled {
      cursor: default;
      {% if settings.button_style == 'full_rounded' or settings.button_style == 'full_square' %}
      background-color: $disabledGrey;
      color: $disabledBorder;
      border: 2px solid $disabledGrey;
      {% else %}
      color: $disabledBorder;
      border: 2px solid $disabledGrey;
      {% endif %}
      display: none;
      .next-arrow {
        display: none;
      }
    }
  }

  .shopify-payment-button__button--hidden {
    display: none;
  }

  &.hide-pay-buttons {
    .shopify-payment-button {
        display: none;
    }
  }


  .AddToCart:not([disabled]) {
    min-height: 44px;
    background: transparent;
    border: 2px solid $colorBtn;
    color: $colorBtn;
    margin-right: ($gutter/2);
    .next-arrow {
      color: $colorBtn;
    }
    border: 2px solid $colorBtn;
    &:hover,
    &:active,
    &:focus {
      outline: none;
      background: transparent;
      color: $colorBtnHover;
      border: 2px solid $colorBtnHover;

      .next-arrow {
        color: $colorBtnHover;
      }
    }
  }
}

.product__store-availability-container {
  margin: 0 0 $gutter;
  position: relative;
}

[data-product-single-media-group] .model-viewer-container {
  position: relative;
  padding-top: 100%;
}

[data-enable-product-zoom="true"] [data-product-media-type-image] {
  cursor: zoom-in;
}

[data-external-video-container] iframe {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.icon-in-stock {
  fill: $successGreen;
  stroke: $successGreen;
}

.icon-out-of-stock {
  fill: $errorRed;
  stroke: $errorRed;
}

$pickup-icon-size: 12px;

.product__store-availability-container .icon {
  width: $pickup-icon-size;
  height: $pickup-icon-size;
}

.pickup-container {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: flex-start;
}

.pickup__icon {
  margin: ($baseFontSize * .0625) ($pickup-icon-size / 2) 0 0;
  line-height: $pickup-icon-size;
}

.pickup__store,
.pickup-expanded__store {
  font-family: $bodyFontStack;
  font-weight: 300;
  font-size: $baseFontSize;
  line-height: $baseFontSize * 1.25;
  margin-bottom: .25em;
}

.pickup__time {
  font-size: .8rem;
}

.pickup__more {
  margin-top: $gutter / 3;
  font-size: .8rem;
}

.pickup__more-link {
  color: $colorSecondary;

  &:hover,
  &:active,
  &:focus {
    color: $colorDrawerAccent;
  }
}

// Styles for async component in drawer

.pickup-expanded__information {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: flex-start;
  margin-bottom: 1em;
}

.pickup-expanded__list-item {
  padding: ($gutter / 2) 0;
}

.pickup-expanded__list-item:not(:first-child) {
  border-top: 1px solid $colorBorder;
}

.pickup-expanded__store {
  font-weight: bold;
}

.pickup-expanded__icon {
  margin: ($baseFontSize * .125) ($pickup-icon-size / 2) 0 0;
  line-height: $pickup-icon-size;
}

.pickup-expanded__information .icon {
  width: $pickup-icon-size;
  height: $pickup-icon-size;
}

.pickup-expanded__details {
  font-size: $baseFontSize;
  line-height: $baseFontSize * 1.25;
  margin-bottom: .25em;
}

.pickup-expanded__address {
  font-style: normal;
}


/*============================================================================
  #Collections
===============================================================================*/

section.collection-page {

  .form-horizontal {
    display: inline-block;
    margin-left: 20px;
    padding: 8px 0;
    @include at-query($max, $small) {
      margin-left: 0;
      display: block;
    }
    label {
      @include secondaryType();
      display: inline-block;
      margin-right: 8px;
      vertical-align: -1px;
      @include at-query($max, $small) {
        display: block;
        margin-bottom: 8px;
      }
    }
    select {
      display: inline-block;
      width: auto;
      @include at-query($max, $small) {
        display: block;
        width: 100%;
        margin-bottom: 8px;
      }
    }
    #SortBy {
      width: auto;
    }
  }

  .collection-header {
    margin-bottom: ($gutter*2);
    padding: 0 0 ($gutter/2) 0;
    border-bottom: 1px solid $colorBorder;

    .standard-index {
      margin: 0;
      &.has-description {
        margin-bottom: $gutter/2;
      }
    }

    @include at-query($max, $small) {
      .grid__item {
        display: block;
        &:first-child {
          margin-bottom: ($gutter/2);
        }
        .collection-description {
          margin-bottom: $gutter;
        }
      }
    }
  }

  .collection__sorting,
  .collection__title {
    vertical-align: bottom;
  }

}


/*============================================================================
  #Newsletter
==============================================================================*/


.newsletter__form {
  margin-top: $gutter;
}

/*============================================================================
  #Map
==============================================================================*/


.map__container {
  position: relative;
  .placeholder-svg {
    border: none;
    min-height: 300px;
  }
}

.grid--table>.grid__item.map__container {
  padding: 0 25px 0 0;
}

.grid--table>.grid__item.map__content {
  padding: 0 0 0 25px;
  text-align: left;
}


.map-wrapper {
  position: relative;
  padding-bottom: 66%;
  overflow:hidden;
}

.map-section__container {
    position: absolute;
    height: 100%;
    min-height: auto;
    left: 0;
    top: 0;
    width: 100%;
}


@include at-query($max,$medium) {
  .grid--table>.grid__item.map__container,
  .grid--table>.grid__item.map__content {
    display: block;
    width: 100%;
    padding: 0;
  }

  .grid--table>.grid__item.map__content {
    text-align: center;
    padding-top: $gutter;
  }
}

/*============================================================================
  #Popup
==============================================================================*/

.popup-container {
  position: fixed;
  bottom: 0;
  right: 0;
  padding: $gutter;
  z-index: 10;
  opacity: 0;
  pointer-events: none;
  transform: translateY(20px);
  transition: all .3s cubic-bezier(0.57, 0.06, 0.05, 0.95);

  &.show {
    opacity: 1;
    pointer-events: auto;
    transform: translateY(0);
  }
}

.popup {
  position: relative;
  max-width: 400px;
  height: auto;
  background: $colorBody;
  border: 1px solid $colorBorder;
  padding: $gutter;
}

.popup__close {
  position: absolute;
  top: $gutter / 3;
  right: $gutter / 2;
  cursor: pointer;

  svg {
    width: 1.25em;
    height: 1.25em;
  }
}

.popup__content {
  margin-top: 0;
  // leave a bit of space
  // on the right-hand side
  max-width: calc(100% - #{$gutter});
}

/*============================================================================
  #Custom
==============================================================================*/
.display-block {
  display: block !important;
}

.text-center {
  text-align: center !important;
}

.hero-image-header-wrap {
  background-color: rgba(0, 0, 0, 0);
  transition: background-color .2s ease-out;
}

.site-header {
  background: #000;

  &.hero-active {
    background: none;
  }
}

.hero-header-fixed {
  position: fixed !important;
  background-color: rgba(0, 0, 0, 1);
}

.btn--header {
  padding: ($gutter / 2) ($gutter / 2) (($gutter / 2) + 1);
}

.header_hero-buttons {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 10px;
  margin: 58px auto 0;
  max-width: 630px;
}

.template-index .hero-active--video {
  position: relative;
  overflow: hidden;
  background-color: #000;
  z-index: 3;

  .feature-image-inner {
    background-image: none !important;
  }

  .video-wrapper {
    width: 100vw;
    height: 56.25vw;
    min-height: 100vh;
    min-width: 177.77vh;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);

    iframe[src] {
      pointer-events: none !important;
    }
  }
}

@include at-query ($min, $medium){
  .template-index .h1--hero {
    font-size: 72px;
    line-height: 84px;
    max-width: 8em;
  }
}
