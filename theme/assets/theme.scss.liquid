    @include at-query($max, $small) {
      padding: 15px 15px 15px 0;
    }
    div.row {
      width: 16px;
      height: 3px;
      margin-bottom: 5px;
      &:last-child {margin-bottom: 0;}
      background-color: $colorHeaderText;
      @include at-query($max, $small) {
        width: 14px;
        height: 2px;
        margin-bottom: 3px;
      }
    }
  }

  .menu-text {
    font-family: $titleFontStack;
    font-weight: $titleFontWeight;
    @include secondaryType();
    color: $colorHeaderText;
    vertical-align: 1px;
    line-height: 16px;
    display: inline-block;
    padding-left: 8px;
