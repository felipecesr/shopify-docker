/*================ COMPONENTS ================*/

/*============================================================================
  #Social Icons
==============================================================================*/


.social-list {
  list-style-type: none;
  margin: 0;
  padding: 0;
  display: inline-block;
  margin-right: 24px;
  vertical-align: 4px;
  li {
    display: inline-block;
    margin: 0 0 0 12px;
    line-height: 1.6;
  }
}

// Header social colors
.social-list {
  font-size: em(16px);
  li {
    a {
      color: $colorHeaderText;
      @include hover($colorHeaderAccent);
    }
  }

  //Alternate header social colors
  .homepage-header-colors & {
    li {
      a {
        color: $colorHomepageHeaderText;
        @include hover($colorHomepageHeaderAccent);
      }
    }
  }
  .homepage-header-colors--white & {
    li {
      a {
        color: #fff;
      }
    }
  }
  .homepage-header-colors--black & {
    li {
      a {
        color: #000;
      }
    }
  }
}




/*============================================================================
  #Disclosure
==============================================================================*/

$max-height-disclosure: 300px;
$min-height-disclosure: 92px;

.disclosure {
  position: relative;
}

.disclosure__toggle {
  @extend select;
  width: auto;
  white-space: nowrap;
  white-space: normal;
  max-width: 25em;
  text-align: right;
}

.disclosure-list {
  background-color: $colorBody;
  border: 2px solid $colorSecondary;
  width: auto;
  min-width: 100%;
  margin: 0;
  bottom: 115%;
  right: 0;
  padding: 1em 0 calc(1em - 1px);
  position: absolute;
  display: none;
  min-height: $min-height-disclosure;
  max-height: $max-height-disclosure;
  overflow-y: auto;
  @include secondaryType();
  {% if settings.button_style == 'full_rounded' or settings.button_style == 'full_square' %}
  border-radius: 3px;
  {% else %}
  border-radius: 0;
  {% endif %}
}

.disclosure-list--visible {
  display: block;
}

.disclosure-list__item {
  border-bottom: 1px solid transparent;
  white-space: nowrap;
  padding: 5px 40px 5px 20px;
  text-align: left;
  margin-bottom: 0;

  &:active {
    background-color: $colorBtn;

    .disclosure-list__option {
      color: $colorBody;
    }
  }
}

.disclosure-list__option {
  color: $colorText;

  &:focus,
  &:hover {
    color: $colorText;
    border-bottom: 2px solid $colorAccent;
  }
}

.disclosure-list__item--current .disclosure-list__option {
  border-bottom: 2px solid $colorAccent;
}

/*============================================================================
  #Pagination
==============================================================================*/
.pagination {
  margin-bottom: 1em;
  text-align: center;

  > span {
    display: inline-block;
    line-height: 1;
  }

  a {
    display: flex;
    color: $colorSecondary;
    align-items: center;

    &:hover {
      color: $colorAccent;
    }

    .next-arrow,
    .prev-arrow {
      font-size: em(20px);
      margin: 0 4px;
      svg {
        display: block;
      }
    }

    &.next-paginate {
      justify-content: flex-end;
    }
  }

  .text-right a {
    justify-content: flex-end;
  }

}


/*============================================================================
  #Site Nav and Dropdowns
==============================================================================*/
.nav-bar {
  background-color: $colorNav;
}

.site-nav, ul.child-nav, ul.child-child-nav {
  @include navigationType();
  cursor: default;
  margin: 0 0 0 0;
  display: inline-block;
  vertical-align: middle;

  &.child-child-nav {
    font-weight: 500;
  }

  li {
    margin: 12px 0;
    display: block;
  }

  a,
  a:hover,
  a:focus {
    color: $colorHeaderText;
    @include hover($colorHeaderAccent);
    text-decoration: none;
  }

  > li {
    position: relative;
    display: inline-block;
    margin: 0 16px 0 0;
    a {
      padding: 8px 8px 8px 0;
    }
  }

  //hide for small screens, as menu icon will always be visible
  @include at-query($max, $medium) {
      display: none;
  }

  .always-burger-menu & {
    display: none;
  }

  .nav-left-only & {
    &.hidden {
      display: none;
    }
  }

}

.homepage-header-colors .site-nav {
  a,
  a:hover,
  a:focus {
    color: $colorHomepageHeaderText;
    @include hover($colorHomepageHeaderAccent);
  }
}

.homepage-header-colors--white .site-nav {
  a,
  a:hover,
  a:focus {
    color: #fff;
  }
}

.homepage-header-colors--black .site-nav {
  a,
  a:hover,
  a:focus {
    color: #000;
  }
}


//expanded styles
.site-nav .site-nav--has-dropdown.nav-hover,
.child-nav .site-nav--has-dropdown-child.nav-hover {
  > a,
  > a:hover,
  > a:focus {
    color: $colorHeaderAccent;
    @include hover($colorHeaderAccent);
  }
}

.homepage-header-colors .site-nav .site-nav--has-dropdown.nav-hover,
.homepage-header-colors .child-nav .site-nav--has-dropdown-child.nav-hover {
  > a,
  > a:hover,
  > a:focus {
    color: $colorHomepageHeaderAccent;
    @include hover($colorHomepageHeaderAccent);
  }
}


/*================ Home to hamburger and cart that toggle drawers ================*/
.site-nav--mobile {
  @extend .site-nav;

  .text-right & {
    margin: 0 (-$gutter / 2) 0 0;
  }
}

.site-nav__link {
  display: block;
  text-decoration: none;
  white-space: nowrap;
  color: $colorNavText;
  cursor: pointer;
  &:hover,
  &:active,
  &:focus {
    color: $colorAccent;
  }


  .icon {
    position: relative;
    top: -3px;
    font-size: 26px;
    padding-left: $gutter / 4;
  }

  .icon-expand_more {
    display: inline-block;
  }

  .icon-expand_less {
    display: none;
  }

  .site-nav--mobile & {
    display: inline-block;
  }
}

.nav-hover {
  .icon-expand_more {
    display: none;
  }

  .icon-expand_less {
    display: inline-block;
  }
}

/*================ Dropdowns ================*/
.site-nav__dropdown {
  display: none;
  position: absolute;
  left: 0;
  margin: 0;
  z-index: $zindexNavDropdowns;
  .supports-no-touch .site-nav--has-dropdown:hover &,
  .site-nav--has-dropdown.nav-hover &,
  .nav-focus + & {
    display: block;
  }
  li a {padding-right: ($gutter);}
}

.site-nav--has-dropdown.nav-hover, .site-nav--has-dropdown.nav-focus  {
  > ul {
    background-color: #fff;
    border: 2px solid $colorHeaderText;
    li {
      margin: 6px 0;
      padding-right: 20px;
    }
    a.site-nav__link {
      font-weight: 500;
      font-size: (18px * 0.8);
      color: $colorBody;

      &:hover,
      &:focus {
        color: $colorHeaderAccent;
        @include hover($colorHeaderAccent);
      }
    }

  }
}

.homepage-header-colors .site-nav--has-dropdown.nav-hover, .site-nav--has-dropdown.nav-focus  {
  > ul {
    a.site-nav__link {
      &:hover,
      &:focus {
        color: $colorHomepageHeaderAccent;
        @include hover($colorHomepageHeaderAccent);
      }
    }
  }
}

/*================ Search bar in header ================*/
.nav-search {
  position: relative;
  padding: 10px 0;

  @include at-query ($max, $medium) {
    padding: 0 0 ($gutter / 2);
    margin: 0 auto;
    text-align: center;
  }
}


/*============================================================================
  #Sub Nav
==============================================================================*/

.site-header .sub-nav,
.site-header .sub-sub-nav {
  max-height: 0;
  display: block;
  overflow: hidden;
  padding: 0;
  @include at-query($max, $medium) {
  display: none !important;
  }

  @include transition(max-height, 0.5s,ease-in-out);

  &.show {
    max-height: 200px;
  }

  ul.child-nav,
  ul.child-child-nav {
    display: none;
    font-size: em(18px);
    position: relative;
    li {
      margin-right: ($gutter/2);
      a {
        padding: 8px 8px 8px 0;
      }
    }
  }

  ul.child-child-nav {
    font-size: em(16px);
  }

  ul.child-list-customer {
  text-align: right;
   height: 45px;
