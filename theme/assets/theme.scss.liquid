  //social
  .social-hero {
    margin-top: $gutter;
    >ul {
      display: none;
      margin: 0;
    }

    .social-list li {
      margin: 0 6px;
    }
  }

  button {
    -webkit-appearance: none;
    background: transparent;
    border: 0;
  }


  .arrow-holder {
    text-align: center;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;

    .arrow {
      cursor: pointer;
      padding: 15px;
      line-height: 0.5;
      font-size: 4em;
      color: $colorHeaderText;
      svg {

      }
      &:focus {outline:0;}
    }

    .arrow:hover svg {
      color: $colorHeaderAccent;
    }

  }


  &.homepage-header-colors {
    .arrow-holder {
      .arrow {
        svg {
          color: $colorHomepageHeaderText;
        }
      }
       .arrow:hover svg {
         color: $colorHomepageHeaderAccent;
      }
    }
  }

  &.homepage-header-colors--white {
    .arrow-holder {
      .arrow {
        svg {
          color: #fff;
        }
      }
    }
  }

  &.homepage-header-colors--black {
    .arrow-holder {
      .arrow {
        svg {
          color: #000;
        }
      }
    }
  }

}

