  #{$class-type}push--#{$namespace}three-sixths    { left: 50%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}four-sixths     { left: 66.666%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}five-sixths     { left: 83.333%; @include silent-relative(); }

  /* Sevenths */
  #{$class-type}push--#{$namespace}one-seventh       { left: 14.2857%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}two-sevenths      { left: 28.57142%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}three-sevenths    { left: 42.85714%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}four-sevenths     { left: 57.14285%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}five-sevenths     { left: 71.42857%; @include silent-relative(); }
  #{$class-type}push--#{$namespace}six-sevenths      { left: 85.71428%; @include silent-relative(); }

  /* Eighths */
  #{$class-type}push--#{$namespace}one-eighth      { left: 12.5%; @include silent-relative(); }
