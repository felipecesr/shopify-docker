      max-width: 100%;
      width: 100%;
    }
  }

  div.header_hero-image {
    height: 100%;

    .header_hero-image-inner {
      height: 100%;
      background-size: cover !important;
      position: relative;
      overflow: hidden;
      z-index: 1;
    }

    .hero-image__img-content {
      width: 100%;
      height: auto;
    }

  }

  //logo
  .logo-hero-image {
    height: 100%;
    text-align: center;
    .site-header__logo {
      //font-size: em($headingHeroFontSize);

      display: inline-block;
      margin-top: -($gutter);

      &.text-active {
        @include siteTitleHeroType();
        a {
          display: inline;
          border-bottom: 6px solid $colorAccent;
          line-height: em(24px);
          span {
            letter-spacing: 0;
          }

          @include at-query ($max, $small) {
            font-size: 26px;
            border-bottom:3px solid $colorAccent;
          }

          &.large-title {
            @include at-query ($max, $desktop) {
              font-size: 56px;
            }

            @include at-query ($max, $medium) {
              font-size: 48px;
            }

            @include at-query ($max, $small) {
              font-size: 24px;
              border-bottom: 4px solid $colorAccent;
            }

          }

          &.small-title {
            @include at-query ($max, $small) {
              font-size: 42px;
              border-bottom: 4px solid $colorAccent;
            }
          }



        }

        @include opacity(0);
        @include transition(opacity,0.2s,ease-in-out);

        &.show {
          @include opacity(1);
        }

      }



      @include at-query ($max, $small) {
