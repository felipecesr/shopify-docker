  ol {
    margin-top: 0;
    margin-bottom: 0;
  }

  li:last-child {
    margin-bottom: 0;
  }

  p {
    margin-bottom: 0;
  }
}

.note {
  border-color: $colorBorder;
}

.errors {
  ul {
    list-style: disc outside;
    margin-left: 20px;
  }
}

.form-success {
  color: $successGreen;
  background-color: $successGreenBg;
  border-color: $successGreen;

  a {
    color: $successGreen;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}

.form-error,
.errors {
  color: $errorRed;
  background-color: $errorRedBg;
  border-color: $errorRed;

  a {
    color: $errorRed;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}



/*============================================================================
  #Cart Page
==============================================================================*/
.cart__row {
  position: relative;
  margin-top: $gutter;
  padding-top: $gutter;
  border-top: 1px solid $colorBorder;
  &.cart__header-labels {
    border-top: none;
  }
  &:first-child {
    margin-top: 0;
    padding-top: 0;
  }
  .js-qty {
    margin: 0 auto;
  }
}

.cart-table {
  th {
    font-weight: normal;
  }
  td,
  th {
    padding: 30px 0;
    border: none;
  }
}

@include at-query ($min, $large) {
  .cart__row--table-large {
    display: table;
    table-layout: fixed;
    width: 100%;
    .grid__item {
      display: table-cell;
      vertical-align: middle;
      float: none;
    }
  }
}

.cart__image {
  display: inline-block;
  vertical-align: middle;
  img {
    display: block;
    max-width: 100%;
  }
  @include at-query($max, $small) {
  text-align: left;
  padding-top: 45px;
    img {
      display: inline-block;
      max-width: 200px;
      width: 100%;
    }
  }
}

.cart__info {
  display: inline-block;
  margin-left: $gutter;
  vertical-align: middle;
  .product-grid-title {
    color: $colorText;
    @include hover($colorAccent);
  }
  .secondary {
    font-weight: 500;
  }
}

@include at-query ($max, $medium) {
  .cart__image, .cart__info {
    display: block;
  }
  .cart__info {
    margin: ($gutter/2) 0 0 0;
    @include at-query($max, $small) {
    text-align: left;
    }
  }
}

.cart-table td.cart__quantity, .cart-table td.data-price {
  padding: 30px 15px;
  @include at-query($max, $small) {
    padding: 30px 0;
  }
}

.data-price {
  text-align: center;
}

.cart__subtotal {
  margin: 0 0 0 ($gutter / 3);
  display: inline;
}

.cart__taxes-and-shipping {
  margin: 0 0 $gutter 0;
}

.cart__mini-labels {
  display: block;
  margin: ($gutter / 3) 0;
  font-size: em(12px);
  @include at-query ($min, $large) {
    display: none;
  }
}

.cart__remove {
  display: block;
}


.highlighted-price {
  color: $colorAccent;
}

.btn--checkout {
  @include at-query ($max, $small) {
    margin-top: 10px;
  }

  @extend .btn--accent;
}

/*============================================================================
  #Add to Cart Button
==============================================================================*/

#AddToCart, .AddToCart {
  span.cart-text {
    display: none;
  }
  #AddToCartText {
    display: inline-block;
  }
  &.is-adding,
  &.is-added {
    #AddToCartText {
      display: none;
    }
    .next-arrow {
      display: none;
    }
  }
  &.is-adding {
    #AddingToCartText {
      display: inline-block;
    }
  }
  &.is-added {
    #AddedToCartText {
      display: inline-block;
    }
  }
}

/*============================================================================
  #Ajax Cart Styles (conditionally loaded)
==============================================================================*/
{% if settings.ajax_cart_enable %}

.ajaxcart__inner {
  margin-bottom: $gutter;
}

.ajaxcart__row {
  > .grid {
    margin-left: -$gutter / 2;

    > .grid__item {
      padding-left: $gutter / 2;
    }
  }
}

.ajaxcart__product {
  position: relative;
  max-height: 500px;

  &.is-removed {
    max-height: 0;
    overflow: hidden;
    visibility: hidden;
    transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
    @include backface();
  }
}

.ajaxcart__row {
  padding-bottom: $gutter / 2;
  margin-bottom: $gutter / 2;
  border-bottom: 1px solid $colorDrawerBorder;
}

.ajaxcart__product-image {
  display: block;
  overflow: hidden;
  margin-bottom: 15px;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }
}

.ajaxcart__product-name,
.ajaxcart__product-meta {
  display: block;
}

.ajaxcart__product-name + .ajaxcart__product-meta {
  padding-top: $gutter / 5;
}

/*================ Quantity Selectors ================*/
.js-qty {
  position: relative;
  margin-bottom: 1em;
  max-width: 100px;
  min-width: 75px;
  overflow: visible;

  input {
    display: block;
    background: none;
    text-align: center;
    width: 100%;
    padding: 5px 25px;
    margin: 0;
  }
}

.js-qty__adjust {
  cursor: pointer;
  position: absolute;
  display: block;
  top: 0;
  bottom: 0;
  border: 0 none;
  padding: 0 8px;
  background: none;
  text-align: center;
  overflow: hidden;
  @include user-select(none);

  &:hover {
    color: $colorAccent;
  }

  svg {
    vertical-align: -3px;
  }
}

.js-qty__adjust--plus {
  right: 0;
  border-left: 2px solid $colorSecondary;
}

.js-qty__adjust--minus {
  left: 0;
  border-right: 2px solid $colorSecondary;
}

/*================ Quantity Selectors in Ajax Cart ================*/
.ajaxcart__qty {
  @extend .js-qty;
  margin: 0;

  .is-loading & {
    opacity: 0.5;
    transition: none;
  }
}

.ajaxcart__qty-num {
  border-color: $colorDrawerBorder;
  color: $colorDrawerText;
}

.ajaxcart__qty-adjust {
  @extend .js-qty__adjust;
  color: $colorDrawerText;
}

.ajaxcart__qty--plus {
  @extend .js-qty__adjust--plus;
  border-color: $colorDrawerBorder;
}

.ajaxcart__qty--minus {
  @extend .js-qty__adjust--minus;
  border-color: $colorDrawerBorder;
}

{% endif %} // settings.ajax_cart_enable

/*================ TEMPLATES ================*/
/*============= Templates | Password =============*/

.template-password {
  height: 100vh;
}

.password-background {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background-size: cover;
}

.password-page {
  display: table;
  height: 100%;
  width: 100%;
  color: $colorText;
  background-size: cover;
  z-index: 2;
  position: relative;

  .ie9 & {
    height: auto;
  }

  .errors,
  .form--success {
    max-width: 500px;
    margin-left: auto;
    margin-right: auto;
  }
}

.password-header {
  height: 85px;
  display: table-row;
}

.password-header__inner {
  display: table-cell;
  vertical-align: middle;
}

.password-login {
  padding: 0 30px;
  text-align: right;
}

.password-logo {
  .logo {
    color: $colorText;
    font-weight: 700;
    max-width: 100%;
  }
}

.password-main {
  display: table-row;
  width: 100%;
  height: 100%;
  margin: 0 auto;
}

.password-main__inner {
  display: table-cell;
  vertical-align: middle;
  padding: ($gutter / 2) $gutter;
}

.password-message {
  max-width: 500px;
  margin: ($gutter * 1.5) auto ($gutter / 2);
}

.password__input-group {
  max-width: 340px;
  margin: 0 auto $gutter;
}

.password__title {
  margin-bottom: $gutter * 1.5;
}

.password__form-heading {
  margin-bottom: $gutter;
}

.password-powered-by {
  margin-top: $gutter * 1.5;
}

.label--hidden {display: none;}

.close-icon {
  &:before {
    font-family: 'icons';
    content: '\e070';
    font-size: 2em;
    line-height: 1.4em;
  }
}

/*================ Modals ================*/

.modal.modal--password {
  @include transform(translateY(-20px));
  background-color: #fff;
  bottom: 0;
  color: $colorText;
  display: none;
  left: 0;
  opacity: 0;
  overflow: hidden;
  position: fixed;
  right: 0;
  top: 0;
  padding: 0;
}

.modal--password.modal--is-active {
  @include transform(translateY(0));
  display: block;
  opacity: 1;
  overflow: hidden;
  z-index: 3;
}

.modal--password .modal__inner {
  @include prefix(transform-style, preserve-3d, moz webkit spec);
  height: 100%;
  padding: 0;
}

.modal--password .modal__centered {
  @include transform(translateY(-50%));
  position: relative;
  top: 50%;

  .no-csstransforms & {
    top: 20%;
  }
}

.modal--password .modal__close {
  border: 0;
  padding: $gutter;
  position: fixed;
  top: 0;
  right: 0;

  .icon {
    font-size: em(20px);
  }
}


/*================ Prevent zoom on touch devices in active inputs ================*/
@include at-query($max, $medium) {
  .template-password .btn,
  input,
  textarea {
    font-size: 16px;
  }

  .template-password .btn {
    margin-top: ($gutter / 2);
  }

  .password__input-group {
    text-align: center;

    .input-group__field,
    .input-group__btn {
      display: block;
    }
  }
}


/*============================================================================
  #Product Grid Items
==============================================================================*/
.grid-uniform.product-grid {

  margin-left: -50px;


  a.product-grid-link {
    display: block;
    position: relative;
    {% unless settings.products_show_second_image_on_hover %}
    @include transition(opacity,0.2s,ease-in-out);
    &:hover {
      @include opacity(0.6);
    }
    {% endunless %}
  }

  .product-grid-on-sale {
    @include productGridType();
    font-size: em(14px * 0.8);
    background: $colorAccent;
    color: $colorBody;
    padding: 5px;
    display: inline-block;
    position: absolute;
    top: 12px;
    right: 12px;
    z-index: 2;
  }

  .grid__item {
    padding-left: 50px;
    margin-bottom: $gutter;
  }

  .grid__image {
    position: relative;
    display: block;
    margin: 0 auto ($gutter / 2);

    img {
      display: block;
      margin: 0 auto;
      {% if settings.product_grid_align == 'left' %}
      margin: 0;
      {% endif %}
    }

  }


  p {
    text-align: {{ settings.product_grid_align }};
    @include productGridType();
    &.h6 {
      margin-bottom: 0;
      span.product-grid-title {
        color: $colorTitle;
        //@include hover($colorAccent);
      }

      span.product-grid-vendor {
        color: $colorSubheader;
      }
    }
    &.price {
      color: $colorSubheader;
    }

    &.unit-price {
      font-size: em(12px);
    }

    s, .sold-out {
      color: $colorSecondary;
      &.sold-out {
        display: inline-block;
      }
    }
  }
}


.homepage-featured-products {
  @include at-query ($min, $large) {
    .grid-uniform.product-grid .grid__item {
      margin-bottom: ($gutter / 2);
    }
  }
  p.price {
    margin-bottom: 0;
  }
}

.product-item-hover {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-size: cover;
  margin-left: auto;
  margin-right: auto;
  background-color: $colorBody;

  @include opacity(0);
  @include transition(all,0.2s,ease-in-out);
}

.product-grid-link:hover .product-item-hover {
  @include opacity(1);
}

.product-grid-item.has-unit-price .price {
  margin-bottom: 0;
}


/*================ SECTIONS ================*/

/*============================================================================
  #Single Product
==============================================================================*/

.template-index section.single-product {
  {% if settings.feature_product_background !=  blank %}
  background: {{ settings.feature_product_background }};
  {% endif %}

  .single-product-image {
    padding: 0 ($gridGutter/2) 0 0;
  }

  @include at-query ($max, $medium) {
    .product-single {
      .grid__item {
        display: table-row;
      }
    }
  }

  .product-single__media {
    margin-bottom: 0;
    @include at-query ($max, $medium) {
      margin-bottom: $gutter;
    }
  }

  .grid--table {
    padding: 30px 0 0;
    {% if settings.single_product_section_title ==  '' %}
    padding: 0;
    {% endif %}
  }

  .single-product-content {
    padding: 0 0 0 ($gridGutter/2);

    h3.standard-single {
      margin-bottom: 0;
      a {
        color: $colorTitle;
        &:hover {
          color: $colorAccent;
        }
      }
    }
    .secondary {
      margin-bottom: 8px;
    }
    .feature-copy {
      margin: 0 0 $gutter;
    }
  }

  ul.product-single__thumbnails {
    text-align: center;
    li {
      a {
        img {
          max-width: 100px;
          width: 100%;
          height: 100%;
        }
      }
    }
  }

}

/*============================================================================
  #Feature Images
==============================================================================*/

.feature-image {
  position: relative;

  &.feature-image--full_screen {
    height: 100vh;
  }

  .wrapper {
    height: 100%;
  }

  .content-wrapper {
    width: 100%;
    height: 100%;
  }

  .feature-image__img-content {
    width: 100%;

    height: auto;
  }

  .feature-image-inner {
    background-size: cover !important;
    background-position: center center;
    padding: $gutter;
    overflow: hidden;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;

    .grid--table {
      display: table;
      table-layout: fixed;
      width: 100%;
      height: 100%;
      padding: 0;

      > .grid__item {
        float: none;
        display: table-cell;
        vertical-align: middle;
        text-align: center;
      }
    }

  }

  .ie8-feature-image {display: none;}

}

.feature-image__placeholder {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #DCDCDC;
  z-index: 0;
  svg {
    width: 100%;
    height: 100%;
    @include opacity(0.3);
  }
  &.feature-image__placeholder--fit {
    position: static;
  }
}

//ie8 fix
.lt-ie9 section.feature-image {
  height: auto;
  .feature-image-inner {
    background: none !important;
    position: relative;

    img {
      display: block;
      width: 100%;
      height: 100%;
    }
    .content-wrapper {
      position: absolute;
      max-width: 100%;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      padding: 0;
    }
  }
}




/*============================================================================
  #Featured Content
==============================================================================*/

section.feature-text {
  .rte {
    margin-bottom: 0;
    > :last-child { margin-bottom: 0 !important; }
  }
}

/*============================================================================
  #Featured Collections
==============================================================================*/

section.featured_collections {
  margin-bottom: -50px;
  .grid__item {
    position: relative;
    display: table;
    .content {
      position: absolute;
      width: 100%;
      height: 100%;
      display: table-cell;
      top: 0;
      left: 0;
      z-index: 2;
      .grid--table {
        display: table;
        table-layout: fixed;
        width: 100%;
        height: 100%;
        padding: $gutter 0;

        > .grid__item {
          float: none;
          padding: 0;
          display: table-cell;
          vertical-align: middle;
          text-align: center;
        }
      }
    }

    img {
      max-width: 100%;
      width: 100%;
      height: auto;
      vertical-align: bottom;
    }

    a {
      display: block;
      position: relative;
      &.onboarding-collection {
        background-color: #5F5F5F;
      }
    }

  }

  .grid-uniform.product-grid .sbs-image {
    margin-bottom: 50px;
  }

  .feature-copy-holder {
    padding: 0 $gutter/2;
    h3.feature-image-overlay {
      margin: 0;
      line-height: 1.2em;
      @include at-query ($max, $desktop) {
        font-size: em(32px);
      }
    }
  }

  .feature-image-cta  {
    margin-top: $gutter;
    @include at-query ($max, $desktop) {
      margin-top: $gutter/2;
    }
  }

}


/*============================================================================
  #Instagram
==============================================================================*/

.instagram {
  .wrapper.grams {
    max-width: 100%;
    padding: 0;
  }
  .gram-item {
    margin: 0;
    padding: 0;
    a {
      display: block;
      @include transition(opacity,0.2s,ease-in-out);
      &:hover {
        @include opacity(0.6);
      }
      img {
        max-width: 100%;
        width: 100%;
        height: auto;
        float: left;
      }
    }
  }
  .gram-item-blank {
    display: none;
  }
  .gram-item__placeholder {
    background-color: #DCDCDC;
  }
}

/*============================================================================
  #Gallery
===============================================================================*/

// Use margins instead of padding so they can collapse
// when galleries are stacked

section.gallery {
  padding: 0 0;
  margin: $gutter 0;

  .section_title {
    padding-top: $gutter;
    margin-bottom: 0;
  }

  &:not(.gallery--flush):not(.gallery--flush-bottom) {
    .grid__item {
      margin-top: $gutter;
      margin-bottom: $gutter;
    }
  }
}

section.gallery--flush {
  margin: 0;
}

section.gallery--flush-bottom {
  margin-bottom: 0;
}

// Allow seamless stacking of galleries on mobile

@include at-query($max, $small) {
  section.gallery {
    padding: 0 0;
    margin: 0 0;
  }

  section.gallery--flush, section.gallery--flush-bottom {
    margin-top: $gutter;
    margin-bottom: $gutter;
  }

  section.gallery:not(.gallery--flush):not(.gallery--flush-bottom) .section_title {
    margin-top: ($gutter * 2);
  }

  section.gallery:not(.gallery--flush):not(.gallery--flush-bottom) {
    .grid__item {
      margin-top: $gutter;
    }
  }
}

.gallery-item {
  display: block;
}

.gallery-image__img-content {
  width: 100%;
  height: auto;
}

.gallery-image-cropped,
.gallery-placeholder--cropped,
.gallery-placeholder-container {
  overflow: hidden;
  position: relative;
}

a[href] {
  .gallery-image,
  .gallery-image-cropped,
  .gallery-placeholder--cropped,
  .gallery-placeholder-container {
    @include transition(opacity,0.2s,ease-in-out);
  }
}

a[href]:hover {
  .gallery-image,
  .gallery-image-cropped,
  .gallery-placeholder--cropped,
  .gallery-placeholder-container {
    @include opacity(0.6);
  }
}

.gallery-image__inner,
.gallery-image-cropped__inner,
.gallery-placeholder__inner {
  padding: $gutter;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;

  .grid--table {
    display: table;
    table-layout: fixed;
    width: 100%;
    height: 100%;
    padding: 0;

    > .grid__item {
      float: none;
      display: table-cell;
      vertical-align: middle;
      text-align: center;
    }
  }
}

.gallery-image-cropped__inner {
  background-size: cover !important;
  background-position: center center;

  .gallery-image-cropped--align-top-left & {
    background-position: top left;
  }

  .gallery-image-cropped--align-top-center & {
    background-position: top center;
  }

  .gallery-image-cropped--align-top-right & {
    background-position: top right;
  }

  .gallery-image-cropped--align-center-left & {
    background-position: center left;
  }

  .gallery-image-cropped--align-center-center & {
    background-position: center center;
  }

  .gallery-image-cropped--align-center-right & {
    background-position: center right;
  }

  .gallery-image-cropped--align-bottom-left & {
    background-position: bottom left;
  }

  .gallery-image-cropped--align-bottom-center & {
    background-position: bottom center;
  }

  .gallery-image-cropped--align-bottom-right & {
    background-position: bottom right;
  }
}

.gallery-caption-text--inner {
  margin-top: 0;
  color: $colorFeatureImageOverlay;

  font-size: em(18px);
  line-height: em(18px);

  p {
    margin-bottom: 0;
  }
}

.gallery-caption-text {
  margin-top: ($gutter / 2);
}

.gallery-placeholder--cropped__svg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}


/*============================================================================
  #Events
==============================================================================*/

section.tour-events {

  .grid--full.grid--table {
    display: table;
    table-layout: fixed;
    width: 100%;
    padding: $gutter 0;
    border-bottom: 1px solid $colorBorder;
    &:last-child {border-bottom: none;}
    &.tour-item-blank {
